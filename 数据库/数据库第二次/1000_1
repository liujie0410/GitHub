/*USE GlobalToyzdeclare @i int set @i=1declare @icOrderno char(6)declare @idOrderDate datetimedeclare @icCartId char(6)declare @icShopperId char(6)declare @icShippingModeId char(2)declare @imShippingCharges moneydeclare @imGiftWrapCharges moneydeclare @icOrderProcessed char(1)declare @imTotalCost moneydeclare @idExpDelDate datetimewhile(@i<=1000) BEGINif(@i<90)set @icOrderno = '0000'+convert(char(2),@i+10)else if((@i>=90)AND(@i<990))set @icOrderno = '000'+convert(char(3),@i+10)else if(@i>=990)set @icOrderno = '00'+convert(char(4),@i+10)declare @BDate1 datetime, @EDate1 datetimeset @BDate1 = '20170520'set @EDate1 = '20170522' set @idOrderDate = DATEADD(second,ABS(CHECKSUM(NEWID()))%DATEDIFF(second,@BDate1,@Edate1),@BDate1)set @icCartId =(select top 1 cCartIdfrom ShoppingCartorder by NEWID())set @icShopperId =(select top 1 cShopperIdfrom Shopperorder by NEWID())set @icShippingModeId =(select top 1 cModeIdfrom ShippingModeorder by NEWID())set @imShippingCharges = ROUND(rand()*100+1,4)set @imGiftWrapCharges = ROUND(rand()*10,4)set @icOrderProcessed = 'Y'set @imTotalCost = ROUND(rand()*3+0.1,4)*100declare @BDate2 datetime, @EDate2 datetimeset @BDate2 = '20170520'set @EDate2 = '20170522'set @idExpDelDate = DATEADD(second,ABS(CHECKSUM(NEWID()))%DATEDIFF(second,@BDate2,@Edate2),@BDate2)insert into Orders values(@icOrderno,@idOrderDate,@icCartId,@icShopperId,@icShippingModeId,@imShippingCharges,@imGiftWrapCharges,@icOrderProcessed,@imTotalCost,@idExpDelDate)set @i = (@i + 1) --循环计数END*/